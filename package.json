{
	"name": "personal-build-frontender",
	"author": "Artur",
	"license": "MIT",
	"version": "1.0.6",
	"description": "Start template with Gulp and Webpack",
	"type": "module",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"keywords": [
		"webpack",
		"gulp",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/PrisonBreak8/personal-build-frontender.git"
	},
	"homepage": "https://github.com/PrisonBreak8/personal-build-frontender",
	"bugs": "https://github.com/PrisonBreak8/personal-build-frontender/issues",
	"scripts": {
		"go": "npx rollup -c",
		"start": "npm i --legacy-peer-deps",
		"dev": "gulp && webpack serve --progress -c config/webpack.dev.js",
		"build": "gulp build --build",
		"sprite": "gulp sprite",
		"fonts": "gulp fonts --rewrite",
		"deploy": "gulp deployFTP --build",
		"zip": "gulp deployZIP --build",
		"devbuild": "gulp build --build --nowebp",
		"tmp": "gulp -f gulpfile.tmp.js",
		"bem": "fls-bemtree-from-html"
	},
	"devDependencies": {
		"@rollup/plugin-babel": "^6.0.4",
